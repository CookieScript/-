local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Unknownkellymc1/Orion/main/source')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "BloxSrap", IntroText = "v1.0.0",HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- notification

OrionLib:MakeNotification({
	Name = "Notification",
	Content = "welcome "..Player.Name.." to BloxSrap",
	Image = "rbxassetid://4483345998",
	Time = 10
})

-- tab1


local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Info"
})

Tab:AddLabel("Username: "..Player.Name.." ")

local LocalizationService = game:GetService("LocalizationService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local countryFlags = {
    PH = "üáµüá≠",
    US = "üá∫üá∏",
    GB = "üá¨üáß",
    IN = "üáÆüá≥",
    -- Add more country codes and flags as needed
}

local success, countryCode = pcall(function()
    return LocalizationService:GetCountryRegionForPlayerAsync(player)
end)

if success then
    local countryFlag = countryFlags[countryCode] or "üè≥Ô∏è" -- Default flag if not found
    Tab:AddLabel("Flag: " .. countryFlag)
else
    print("GetCountryRegionForPlayerAsync failed: " .. countryCode)
end


-- tab2


local Tab = Window:MakeTab({
	Name = "Mods",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Mods"
})


Tab:AddButton({
	Name = "Stretch screen",
	Callback = function()


local Camera = workspace.CurrentCamera
if getgenv().gg_scripters == nil then
    game:GetService("RunService").RenderStepped:Connect(
        function()
            Camera.CFrame = Camera.CFrame * CFrame.new(0, 0, 0, 1, 0, 0, 0, getgenv().Resolution[".gg/scripters"], 0, 0, 0, 1)
        end
    )
end
getgenv().gg_scripters = "Aori0001"


  	end    
})


Tab:AddSlider({
	Name = "Resolution",
	Min = 0,
	Max = 1,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.01,
	ValueName = "Value",
	Callback = function(Value)
	        getgenv().Resolution = {
    [".gg/scripters"] = Value
}
	end    
})

-- tab3

local Tab = Window:MakeTab({
	Name = "Fastscript",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Wip"
})


Tab:AddTextbox({
    Name = "Fastscript",
    Default = "",
    TextDisappear = false,
    Callback = function(Value)
        print(Value)
        currentScript = Value
    end	  
})

Tab:AddButton({
    Name = "Add Fastscript",
    Callback = function()
        if currentScript then
            local success, err = pcall(loadstring(currentScript))
            if not success then
                game.StarterGui:SetCore("SendNotification", {
                Title = "Notification",
                Text = "Error Fastscript Loading"..er})
            end
        else
            game.StarterGui:SetCore("SendNotification", {
            Title = "Notification",
            Text = "No script provide" })
        end
    end    
})


Tab:AddLabel("How to use. get a link or script make sure the link")
Tab:AddLabel("has a script then put it in Fastscript Click Add Fast script")





